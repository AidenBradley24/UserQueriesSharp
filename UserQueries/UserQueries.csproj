<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>UserQueriesSharp</PackageId>
    <Version>0.1.0</Version>
    <Authors>Aiden Bradley</Authors>
    <Description>A library that allows users to interface with an IQueryable.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/AidenBradley24/UserQueriesSharp</RepositoryUrl>
    <PackageProjectUrl>https://github.com/AidenBradley24/UserQueriesSharp</PackageProjectUrl>
  </PropertyGroup>

</Project>
